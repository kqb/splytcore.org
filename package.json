{
  "name": "bakery",
  "version": "0.1.0",
  "description": "Frontend Boilerplate - A Webpack based FrontEnd Starter kit supporting ES6, SCSS and Handlebars",
  "main": "index.js",
  "scripts": {
    "create": "node presets.js",
    "dev": "webpack-dev-server",
    "dev:open": "webpack-dev-server --open",
    "build": "webpack",
    "watch": "webpack --watch",
    "deploy": "aws s3 sync dist/ s3://www.splytcore.org --acl public-read",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E1FHGYFXO0FEXE --paths '/*'",
    "production": "yarn build && yarn deploy && yarn invalidate"
  },
  "author": "Denni Bevilacqua",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "glob": "^7.1.4",
    "handlebars": "^4.3.0",
    "handlebars-loader": "^1.7.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "img-loader": "^3.0.2",
    "inline-manifest-webpack-plugin": "^4.0.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "preload-webpack-plugin": "3.0.0-beta.3",
    "sass-loader": "^7.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.8.0",
    "webpack-manifest-plugin": "^3.1.0",
    "webpack-nano": "^1.1.1"
  },
  "dependencies": {
    "babel-plugin-transform-es2015-modules-strip": "^0.1.2",
    "bootstrap": "4.0.0",
    "gsap": "^3.6.0",
    "jquery": "^3.6.0",
    "lazysizes": "^5.3.0",
    "popper": "^1.0.1",
    "popper.js": "^1.12.9",
    "progressive-image.js": "^1.6.0",
    "scrollreveal": "^4.0.9",
    "three": "^0.126.1",
    "tilt.js": "^1.2.1",
    "vunit.js": "^0.2.0"
  }
}
